{
  "extends": [
    "4catalyzer",
    "4catalyzer-jest",
    "prettier",
    "4catalyzer-typescript"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-empty-function": "off",
    "@typescript-eslint/no-empty-function": [
      "error",
      {
        "allow": ["constructors"]
      }
    ]
  }
}
